{
  "name": "@raysx/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=19"
  },
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development yarn ts-node-dev server",
    "dev:client": "vite --config vite.config.ts dev --host",
    "start": "yarn dev:client",
    "build": "rimraf dist && tsc -p tsconfig.prod.json && yarn build:client && yarn build:server && yarn docker:build && yarn docker:push",
    "action": "rimraf dist && yarn build:client && yarn build:server && yarn ssg",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "prerender": "cross-env NODE_ENV=production yarn ts-node prerender.cts",
    "serve": "cross-env NODE_ENV=production yarn node dist/server.mjs",
    "ssg": "serve dist/client",
    "codegen": "yarn codegen:uniswap && yarn codegen:pancake",
    "codegen:uniswap2": "graphql-codegen --config src/graphql/codegen-uniswap2.ts",
    "codegen:uniswap3": "graphql-codegen --config src/graphql/codegen-uniswap3.ts",
    "codegen:pancake": "graphql-codegen --config src/graphql/codegen-pancake.ts",
    "docker:build": "docker buildx build --platform linux/amd64 -t bardakdev/ray-client . --no-cache",
    "docker:push": "docker push bardakdev/ray-client",
    "docker:build:nginx": "docker buildx build --platform linux/amd64 -t ray-nginx-local . --no-cache",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@0x/utils": "^7.0.0",
    "@apollo/client": "3.7.1",
    "@fancyapps/ui": "4.0.31",
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@react-spring/web": "9.5.5",
    "@types/compression": "1.7.2",
    "@viaprotocol/router-sdk": "^1.0.7",
    "animate.css": "4.1.1",
    "axios": "1.1.3",
    "bignumber.js": "^9.1.1",
    "clipboard": "2.0.11",
    "clsx": "1.2.1",
    "color": "4.2.3",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "2.29.3",
    "ethers": "^5.7.2",
    "graphql": "16.6.0",
    "lightweight-charts": "3.8.0",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "millify": "5.0.1",
    "mobx-react-lite": "3.4.0",
    "normalize.css": "8.0.1",
    "numeral": "2.0.6",
    "p-all": "5.0.0",
    "p-throttle": "5.1.0",
    "polished": "^4.2.2",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-redux": "8.0.5",
    "react-resize-observer": "1.1.1",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scroll": "1.8.8",
    "react-spring": "9.5.5",
    "react-table": "7.8.0",
    "react-use-websocket": "4.2.0",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.8",
    "recharts": "2.1.16",
    "redux": "4.2.0",
    "sass": "1.56.0",
    "serve": "14.0.1",
    "slick-carousel": "1.8.1",
    "styled-components": "5.3.6",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
