{
  "name": "@raysx/frontend",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=19"
  },
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development yarn ts-node-dev server",
    "dev:client": "vite --config vite.config.ts dev --host",
    "start": "yarn dev:client",
    "build": "rimraf dist && tsc -p tsconfig.prod.json && npm run build:client && npm run build:server && yarn prerender",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "prerender": "cross-env NODE_ENV=production yarn ts-node prerender.cts",
    "serve": "cross-env NODE_ENV=production yarn node dist/server.mjs",
    "ssg": "serve dist/client",
    "format": "prettier --write .",
    "codegen": "yarn codegen:uniswap && yarn codegen:pancake",
    "codegen:uniswap": "graphql-codegen --config src/graphql/codegen-uniswap.ts",
    "codegen:pancake": "graphql-codegen --config src/graphql/codegen-pancake.ts",
    "docker:build": "docker buildx build --platform linux/amd64 -t bardakdev/ray-client . --no-cache"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@fancyapps/ui": "4.0.31",
    "@react-spring/web": "9.5.5",
    "@types/compression": "1.7.2",
    "animate.css": "4.1.1",
    "axios": "1.1.3",
    "clipboard": "2.0.11",
    "clsx": "1.2.1",
    "color": "4.2.3",
    "date-fns": "^2.29.3",
    "graphql": "16.6.0",
    "lightweight-charts": "^3.8.0",
    "lodash": "4.17.21",
    "lodash-es": "^4.17.21",
    "millify": "5.0.1",
    "mobx-react-lite": "3.4.0",
    "normalize.css": "8.0.1",
    "numeral": "2.0.6",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-resize-observer": "^1.1.1",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scroll": "^1.8.8",
    "react-select": "5.6.0",
    "react-spring": "9.5.5",
    "react-table": "7.8.0",
    "react-use-websocket": "4.2.0",
    "recharts": "^2.1.16",
    "redux": "4.2.0",
    "sass": "1.56.0",
    "serve": "14.0.1",
    "slick-carousel": "1.8.1",
    "styled-components": "5.3.6",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/client-preset": "1.0.7",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-operations": "2.2.4",
    "@graphql-codegen/typescript-react-apollo": "3.2.17",
    "@graphql-codegen/typescript-react-query": "4.0.5",
    "@types/express": "4.17.14",
    "@types/lodash": "4.14.188",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-react": "2.2.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "4.18.2",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "serve-static": "1.15.0",
    "typescript": "4.8.4",
    "vite": "3.2.3"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8"
  }
}
